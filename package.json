{
  "name": "eco-training-disaster",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "frontend": "vite --port 3000 --host",
    "backend": "NODE_OPTIONS='--max-old-space-size=4096' node backend/server.js",
    "build": "npm run build:frontend",
    "build:frontend": "vite build --mode production",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "lint": "eslint . --ext .ts,.tsx --ignore-pattern backend/ --fix",
    "lint:check": "eslint . --ext .ts,.tsx --ignore-pattern backend/",
    "preview": "vite preview --port 4173",
    "clean": "rm -rf dist node_modules/.cache",
    "start": "node backend/server.js",
    "test": "echo \"Tests not configured\"",
    "eco:analyze": "node analyse-ecoperformance.js",
    "eco:build": "npm run clean && npm run build && npm run eco:analyze"
  },
  "dependencies": {
    "@types/three": "^0.162.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.162.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "eco-design",
    "rgsen",
    "performance",
    "optimization",
    "react",
    "vite",
    "typescript"
  ],
  "author": "EcoTraining Team",
  "description": "Plateforme d'entraînement éco-responsable optimisée selon les bonnes pratiques RGESN",
  "repository": {
    "type": "git",
    "url": "https://github.com/yabarji-startUp/Disasters-web.git"
  },
  "license": "MIT"
}
